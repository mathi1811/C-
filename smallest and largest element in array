#include<iostream>
using namespace std;
int main()
{
    int n,i,arr[10],min,max;
    cout<<"Enter the size of array:";
    cin>>n;
    cout<<"The elements in array are:";

    for(i=0;i<n;i++)
     cin>>arr[i];
    max=arr[0];
    for(i=0;i<n;i++)
    {
        if(max<arr[i])
        max=arr[i];
    }
    min=arr[0];
    for(i=0;i<n;i++)
    {
        if(min>arr[i])
        min=arr[i];
    }
    cout<<"Largest element is:"<<max;
    cout<<"Smallest element is:"<<min;
    return 0;
    
}

