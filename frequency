#include<iostream>
using namespace std;
int frequency(int A[],int n,int x)
{
    int count=0;
    for(int i=0;i<n;i++)
    if(A[i]==x)
    count++;
    return count;
}
int main()
{
    int A[]={1,1,2,4,5,2,3,7,4,1,3};
    int x=1;
    int n=sizeof(A)/sizeof(A[0]);
    cout<<frequency(A,n,x);
    return 0;
}
