#include<iostream>
using namespace std;
int main()
{
    int n;
    cout<<"Enter the size of the array";
    cin>>n;
    int i,array[n];
    for(i=1;i<=n;i++)
    {
    cout<<"Enter the elements in array";
    cin>>array[i];
    }
    int odd=0,even=0;
    for(i=1;i<=n;i++)
    {
        if(array[i]%2==0)
        {
            even++;
        }
        else
        {
            odd++;
        }
    }    
        if(odd==n)
        {
            cout<<"ODD type array";
        }
        else if(even==n)
        {
            cout<<"EVEN type array";
        }
        else
        {
            cout<<"MIXED type array";
        }
        return 0;
    }
    
