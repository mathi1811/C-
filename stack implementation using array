#include<iostream>
using namespace std;
int stack[100],n=100,top=-1;
void push(int val)
{
    if(top>=n-1)
    {
        cout<<"\n OVERFLOW";
    }
    else
    {
        top++;
        stack[top]==val;
    }
}
void pop(int val)
{
    if(top<=-1)
    {
        cout<<"UNDERFLOW";
    }
    else
    {
        top--;
        stack[top]=val;
    }
}
void display()
{
    if(top>=0)
    {
        int i;
        cout<<"Stack elements are:";
        for(int i=top;i>=0;i--)
            cout<<stack[i]<<" ";
    }
        else
            cout<<"Stack is empty";
}
int main()
{
    int ch,val;
    cout<< "1.PUSH 2.POP 3.DISPLAY 4.EXIT";
    do
    {
    cout<<"\nEnter your choice";
    cin>>ch;
    switch(ch)
    {
        case 1:
        {
        cout<<"enter value:";
        cin>>val;
        push(val);
        break;
        }
        case 2:
        {
            cout<<"Enter value:";
            cin>>val;
            pop(val);
            break;
        }
        case 3:
        {
            display();
            break;
        }
        case 4:
        {
        cout<<"Exit";
        break;
        }
        default:
        cout<<"Enter valid number!";
        break;
        
    }
}while(ch!=4);
    return 0;
    
}
